{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "max-line-length":[true, 200],
        "no-var-requires": false,
        "no-console": [
            false,
            "log",
            "error"
